{
  "name": "astrofy",
  "description": "Astrofy is a free and open-source template for your Personal Portfolio Website built with Astro and TailwindCSS. Create in minutes a website with Blog, CV, Project Section, Store and RSS Feed.",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "imports": {
    "#layouts/*.astro": "./src/layouts/*.astro",
    "#components/*.astro": "./src/components/*.astro",
    "#lib/*": "./src/lib/*.ts",
    "#config": "./src/config.mts"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "exports": {
    "./components/*": "./src/components/*",
    "./layouts/*": "./src/layouts/*"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.13.3",
    "daisyui": "^4.12.10",
    "dayjs": "^1.11.12",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.9"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.14",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "^20.3.0",
    "pnpm": ">=9.1.2"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
}
