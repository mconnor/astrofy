{
  "name": "astrofy",
  "description": "Astrofy is a free and open-source template for your Personal Portfolio Website built with Astro and TailwindCSS. Create in minutes a website with Blog, CV, Project Section, Store and RSS Feed.",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "imports": {
    "#*": "./src/*",
    "#config": "./src/config.mts",
    "#contentConfig": "./src/content/config.ts"
  },
  "scripts": {
    "check": "astro check",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "inspect": "eslint  --inspect-config",
    "lint": "eslint",
    "fix": "eslint --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.1",
    "astro": "^4.15.11",
    "daisyui": "^4.12.11",
    "dayjs": "^1.11.13",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.3",
    "@eslint/js": "^9.11.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "astro-eslint-parser": "^1.0.3",
    "eslint": "~9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-markdown": "^5.1.0",
    "globals": "^15.10.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  },
  "engines": {
    "node": ">= 20.11.0 <21 || >= 22.9.0"
  },
  "packageManager": "pnpm@9.12.0"
}
