{
  "name": "astrofy",  
  "author": "Mike Connor",
  "description": "Astrofy is a free and open-source template for your Personal Portfolio Website built with Astro and TailwindCSS. Create in minutes a website with Blog, CV, Project Section, Store and RSS Feed.",
  "type": "module",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "check": "astro check",
    "build": "astro check astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"src/**/*.{js,jsx,ts,jsx,astro,md}\"",
    "fix": "eslint \"src/**/*.{js,jsx,ts,jsx,astro,md}\" --fix",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "stylelint": "stylelint \"src/**/*.{css,astro}\"",
    "sfix": "stylelint \"src/**/*.{css,astro}\" --fix",
    "inspect": "eslint --inspect-config"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.0.0",
    "@astrojs/rss": "^4.0.6",
    "@astrojs/sitemap": "^3.1.4",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.8.6",
    "daisyui": "^4.11.1",
    "dayjs": "^1.11.11",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.3.0",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-regexp": "^2.6.0",
    "globals": "^15.3.0",
    "postcss-html": "^1.7.0",
    "postcss-import": "^16.1.0",
    "postcss-jit-props": "^1.0.14",
    "postcss-nesting": "^12.1.4",
    "postcss-pow": "^1.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "stylelint": "^16.5.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
